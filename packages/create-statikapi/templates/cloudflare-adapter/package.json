{
  "name": "APP_NAME",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "statikapi-cf --src src-api --out dist/worker.mjs",
    "watch": "chokidar \"src-api/**/*.{js,mjs,cjs}\" -c \"pnpm build\"",
    "wrangler:dev": "wrangler dev --local",
    "dev": "concurrently -n BUILD,WRANGLER -c yellow,cyan \"pnpm watch\" \"pnpm wrangler:dev\""
  },
  "devDependencies": {
    "@statikapi/adapter-cf": "^0.6.2",
    "wrangler": "^4.47.0",
    "concurrently": "^9.0.0",
    "chokidar-cli": "^3.0.0"
  }
}
